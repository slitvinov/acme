w=157
fn c {	
	echo -n Edit ,d | winwrite body
	echo -n /Edit ,d/ |  winwrite addr
	echo -n 'dot=addr' | winwrite ctl
	winread addr | awk '{ printf "MX%s %s\n", $1, $2}' | winwrite event
}

